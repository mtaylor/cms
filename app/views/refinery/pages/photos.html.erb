<!-- Start Advanced Gallery Html Containers -->
<div id="content">
	<div id="images">
	</div>
</div>

<script type="text/javascript">
	jQuery(document).ready(function($) {
		FB.api("/"+642411042443138+"/photos/?limit=200",function(response)
		{
			console.log(response["data"]);
		    var photos = response["data"];
		    for(var v=0;v<photos.length;v++)
		    {
		    	$("#images").append("<a class='gallery-thumbs' rel='shadowbox[HFHR]' href='" + photos[v].images[0].source + "'><img src='" + photos[v].images[6].source + "' alt='Picture' /></a>");
		    }
		});
		Shadowbox.init({
          handleOversize: "drag",
          modal: true,
          continuous: true,
          viewportPadding: 50
        });
	});
</script>